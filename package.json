  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "detox": "^9.1.2",
    "haul": "^1.0.0-rc.10",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.49.2",
    "react-hot-loader": "^4.3.12",
    "react-test-renderer": "16.6.1"
  },
  "jest": {
    "preset": "react-native",
